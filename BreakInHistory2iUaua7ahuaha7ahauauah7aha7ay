    for i = 1, 10 do
				KillEnemies()
			end
end)

TabN:Toggle("ğŸŒ€ â€¢ Loop Kill All Enemies",false,function(Value)
getgenv().KillAllLoop = Value
			while KillAllLoop do
				for i = 1, 3 do
					KillEnemies()
				end
				task.wait(.1)
			end
    end)

